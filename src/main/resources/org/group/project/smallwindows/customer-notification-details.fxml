<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="TOP_CENTER"
      maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
      fx:id="vbox"
      fx:controller="org.group.project.controllers.customer.CustomerNotificationDetailsController"
      stylesheets="@../css/small-windows.css"
>
    <VBox styleClass="labelBox">
        <Label fx:id="dateStampLabel" styleClass="dateStampLabel"
               text="Date Stamp Here"/>
        <Label fx:id="subjectLabel" styleClass="highlightLabel"
               text="Subject Stamp"/>
    </VBox>
    <VBox styleClass="body-textareaBox">
        <TextArea fx:id="notificationTextArea"
                  styleClass="body-textarea"/>
    </VBox>
    <HBox alignment="CENTER" prefHeight="49.0" prefWidth="600.0"
          stylesheets="@../css/two-button-box.css" styleClass="buttonBox">
        <Button fx:id="returnButton" mnemonicParsing="false"
                styleClass="cancelButton" text="Return"/>
    </HBox>
</VBox>
